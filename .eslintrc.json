{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": ["error", "windows"], 
        "prefer-destructuring": ["error", {
            "array": false,
            "object": false
          }, {
            "enforceForRenamedProperties": false
          }],
        "no-console": ["error", { "allow": ["warn", "error", "log"] }]
    }
}
